<!DOCTYPE html>
<html>

<head>
  <title>Beach finder</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <style>
    #map {
      height: 500px;
      width: 100%;
    }
  </style>
</head>

<body>
  <h1>Beach finder</h1>
  <span>Beach:</span>
  <input id="autocomplete" type="text">
  <button type="button">Search</button>
  <div id="map"></div>
  <script>
    function startMap() {
      var map = new google.maps.Map(document.getElementById('map'), {
        center: { lat: 25.825986, lng: -80.182716 },
        zoom: 15,
      });
    }
  </script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD8h-hiGFaCuJTAuSP2_KT4ys_0URtS2oU&libraries=places&callback=startMap"
    async defer> </script>

</body>

</html>

